{
  "name": "habitica-magicwand",
  "version": "1.0.0",
  "description": "Test",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:clean": "rimraf build/*",
    "build:copy": "copyfiles -u 1 source/manifest.json source/images/* source/scripts/*.js build",
    "build:pug:main": "pug source/*.pug -o build/",
    "build:pug:views": "pug source/views/*.pug -o build/views",
    "build:pug": "npm run build:pug:main -s && npm run build:pug:views -s",
    "prebuild": "npm run build:clean -s",
    "build": "npm run build:copy -s && npm run build:pug -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrTyx/Habitica-Autocast.git"
  },
  "author": "MrTyx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrTyx/Habitica-Autocast/issues"
  },
  "homepage": "https://github.com/MrTyx/Habitica-Autocast#readme",
  "devDependencies": {
    "angular": "^1.6.4",
    "bootstrap": "^3.3.7",
    "colors": "^1.1.2",
    "copyfiles": "^1.2.0",
    "font-awesome": "^4.7.0",
    "node-zip": "^1.1.1",
    "pug": "^2.0.0-rc.1",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.1"
  }
}
