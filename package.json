{
  "name": "habitica-magicwand",
  "version": "1.0.0",
  "description": "Test",
  "main": "app.js",
  "scripts": {
    "build:copy": "copyfiles -u 1 source/manifest.json source/images/* source/scripts/*.js build",
    "build:pug": "pug source/*.pug --out build/ --pretty",
    "build:browserify": "cross-env NODE_ENV=production browserify -g envify source/app.js | uglifyjs -c warnings=false -m > build/app.min.js",
    "prebuild": "rimraf build/*",
    "build": "npm-run-all build:copy build:pug build:browserify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrTyx/Habitica-MagicWand.git"
  },
  "author": "MrTyx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrTyx/Habitica-MagicWand/issues"
  },
  "homepage": "https://github.com/MrTyx/Habitica-MagicWand#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "bootstrap": "^3.3.7",
    "browserify": "^14.3.0",
    "colors": "^1.1.2",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.0.0",
    "envify": "^4.0.0",
    "font-awesome": "^4.7.0",
    "node-zip": "^1.1.1",
    "pug": "^2.0.0-rc.1",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.1",
    "uglify-js": "^3.0.4",
    "vueify": "^9.4.1"
  },
  "dependencies": {
    "keen-ui": "^1.0.0",
    "vue": "^2.3.3"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}
